<resources>


    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/green_600</item>
        <item name="colorPrimaryDark">@color/green_600</item>
        <item name="colorAccent">@color/green_600</item>
        <item name="textColorError">@color/Red</item>
    </style>

    <style name="AppThemeInput" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/green_600</item>
        <item name="colorPrimaryDark">@color/green_600</item>
        <item name="colorAccent">@color/md_white</item>
        <item name="textColorError">@color/Red</item>
    </style>
    <style name="AppInputBlue" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/green_600</item>
        <item name="colorPrimaryDark">@color/green_600</item>
        <item name="colorAccent">@color/green_600</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppThemeInput.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="AppInputBlue.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="text_extralarge_bold">
